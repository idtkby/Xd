})

local Tab = Window:CreateTab("Trench War", 4483362458)
local Section = Tab:CreateSection("Main", false)

Tab:CreateButton({
    Name = "no cooldown",
    SectionParent = Section,
    Interact = "Run",
    Callback = function()
        g = hookfunction(wait, function(seconds)
            return g(0)
        end)
        
    end
})

Tab:CreateButton({
    Name = "Hitbox Gui",
