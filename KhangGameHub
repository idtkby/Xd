        Note = "Ytub: KhangRobloxScr",
        FileName = "KGameHKeys",
        SaveKey = true,
        GrabKeyFromSite = false,
        AutoCheck = true,
        Key = {"Hello", "Bye"},
        Actions = {
            [1] = {
                Text = 'Click here to getkey',
                OnPress = function()
                    setclipboard("Hello")
                end,
            }
        },
    }
})

local Tab = Window:CreateTab("Trench War", 4483362458)
local Tab = Tab:CreateSection("Main", false)

Tab:CreateButton({
    Name = "No cooldown",
    SectionParent = Tab,
    Interact = "Run",
    Callback = function()
        g = hookfunction(wait, function(seconds)
            return g(0)
        end)
        
    end
})

Tab:CreateButton({
    Name = "Hitbox Gui",
    SectionParent = Tab,
    Interact = "Run",
    Callback = function()
        loadstring(game:HttpGet("https://scriptblox.com/raw/Universal-Script-hitbox-expander-real-good-7163"))()
    end
})

