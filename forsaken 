	UIStroke.Thickness = 1.5
	UIStroke.Parent = GuiGenEspText
	elseif _G.EspGui == false and v:FindFirstChild("Esp_Gui") then
	v:FindFirstChild("Esp_Gui"):Destroy()
end
end
end
