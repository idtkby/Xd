})

-- after your Esp_Player section, add:

-- helper to create ESP on a single model
local function CreateItemESP(model: Model, color: Color3, labelText: string)
    if model:FindFirstChild("ItemESP_Box") then return end

    -- Box outline
    local box = Instance.new("BoxHandleAdornment")
    box.Name = "ItemESP_Box"
    box.Adornee = model.PrimaryPart or model:FindFirstChildWhichIsA("BasePart")
    box.Size = model:GetExtentsSize() + Vector3.new(0.5, 0.5, 0.5)
    box.AlwaysOnTop = true
    box.ZIndex = 10
    box.Color3 = color
    box.Transparency = 1
    box.Parent = model

    -- Label
    local head = model:FindFirstChildWhichIsA("BasePart")
    if head then
        local gui = Instance.new("BillboardGui", head)
        gui.Name = "ItemESP_Gui"
        gui.Adornee = head
        gui.Size = UDim2.new(0, 100, 0, 40)
        gui.StudsOffset = Vector3.new(0, 2, 0)
        gui.AlwaysOnTop = true

        local lbl = Instance.new("TextLabel", gui)
        lbl.Size = UDim2.new(1, 0, 1, 0)
        lbl.BackgroundTransparency = 1
        lbl.Font = Enum.Font.Code
        lbl.TextSize = 14
        lbl.TextColor3 = color
        lbl.Text = labelText
        -- optional stroke:
        local stroke = Instance.new("UIStroke", lbl)
        stroke.Color = Color3.new(0, 0, 0)
        stroke.Thickness = 1
    end
end

-- helper to clear ESP from a model
local function ClearItemESP(model: Model)
    if model:FindFirstChild("ItemESP_Box") then
        model.ItemESP_Box:Destroy()
    end
    if model.PrimaryPart then
        local gui = model.PrimaryPart:FindFirstChild("ItemESP_Gui", true)
        if gui then gui:Destroy() end
    end
end

-- BloxyCola toggle
Main2Group:AddToggle("EspBloxyCola", {
    Text = "ESP BloxyCola",
    Default = false,
    Callback = function(on)
        spawn(function()
            while on do
                for _, mdl in ipairs(workspace:GetDescendants()) do
                    if mdl:IsA("Model") and mdl.Name == "BloxyCola" then
                        CreateItemESP(mdl, Color3.fromRGB(0, 170, 255), "BloxyCola")
                    end
                end
                wait(1)
            end
            -- clear when off
            for _, mdl in ipairs(workspace:GetDescendants()) do
                if mdl:IsA("Model") and mdl.Name == "BloxyCola" then
                    ClearItemESP(mdl)
                end
            end
        end)
    end
})

