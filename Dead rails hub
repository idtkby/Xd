OtherTab:CreateButton({
    Name = "RareHorse Notice",
    Callback = function()
        local GuiService = game:GetService("StarterGui")
local workspace = game:GetService("Workspace")

local horseIDs = {
    Model_WarHorse = 98866039236398,
    Model_DeathHorse = 117026495479263,
    Model_PestilenceHorse = 137680581466072,
    Model_FamineHorse = 72130171723716,
}

-- Hàm thông báo
local function notifyHorse(name, imageId)
    GuiService:SetCore("SendNotification", {
        Title = "[!] Apocalypse Horse Spawned!",
        Text = name .. " has appeared in the world!",
        Duration = 6,
        Icon = "rbxthumb://type=Asset&id=" .. imageId .. "&w=150&h=150"
    })
end

-- Theo dõi workspace khi có model được thêm
workspace.ChildAdded:Connect(function(child)
    if horseIDs[child.Name] then
        local horseName = child.Name:gsub("Model_", ""):gsub("Horse", "") -- Lấy tên rút gọn
        notifyHorse(horseName, horseIDs[child.Name])
    end
end)
    end,
})

end)
