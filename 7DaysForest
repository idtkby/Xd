KTab:CreateSection(L("Save location"))
local originalPos = nil

KTab:CreateButton({
    Name = L("Save current location"),
    Callback = function()
        local hrp = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if hrp then
            originalPos = hrp.CFrame
            Rayfield:Notify({
                Title = L("Original location"),
                Content = L("Current location saved"),
                Duration = 2
            })
        end
    end,
})

KTab:CreateButton({
    Name = L("Return to original position"),
    Callback = function()
        local hrp = game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
        if originalPos and hrp then
            hrp.CFrame = originalPos
            Rayfield:Notify({
                Title = L("Teleport"),
                Content = L("Teleported to saved location"),
                Duration = 2
            })
        end
    end,
})
  
end)

task.wait(1) -- Đợi Rayfield load hoàn tất
local function applyFontToRayfieldFonts()
    for _, gui in pairs(game:GetService("CoreGui"):GetDescendants()) do
        if gui:IsA("TextLabel") or gui:IsA("TextButton") or gui:IsA("TextBox") then
            pcall(function()
                gui.Font = Enum.Font.Bodoni
            end)
        end
    end
end

applyFontToRayfieldFonts()












local LTab = Window:CreateTab("Language", "setting")

LTab:CreateDropdown({
	Name = "Ngôn ngữ / Language",
	Options = {"Tiếng Việt", "English"},
	CurrentOption = {"English"},
	MultipleOptions = false,
	Flag = "LanguageOption",
	Callback = function(option)
		if option[1] == "Tiếng Việt" then
			currentLanguage = "vi"
		else
			currentLanguage = "en"
		end

		Rayfield:Notify({
			Title = L("Main"),
			Content = currentLanguage == "vi" and "Đã đổi sang Tiếng Việt" or "Switched to English",
			Duration = 3
		})
rebuildMainTab() -- Gọi lại tab sau khi đổi ngôn ngữ
	end
})