local CTab = Window:CreateTab("Craft", "wrench")
local lp = game.Players.LocalPlayer

CTab:CreateSection("Ingredient")
local woodLabel = CTab:CreateLabel("WOOD: ...")

task.spawn(function()
	local stats = lp:WaitForChild("stats", 10)
	if not stats then
		woodLabel:Set("Woodü™µ: [stats not found]")
		return
	end

	local wood = stats:WaitForChild("wood", 10)
	if not wood then
		woodLabel:Set("Woodü™µ: [wood not found]")
		return
	end

	woodLabel:Set("Woodü™µ: " .. tostring(wood.Value))

	wood:GetPropertyChangedSignal("Value"):Connect(function()
		woodLabel:Set("Woodü™µ: " .. tostring(wood.Value))
	end)
end)

